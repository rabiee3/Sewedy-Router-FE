    <header>
        <name>Line Calling Feature</name>
		<image>images/icons/voip.png</image>
        <description>This page allows to configure advanced calling features.</description>
    </header>
    <object viewtype="table" title="Line Calling Feature">
        <object  name="Device.Services.VoiceService.1.VoiceProfile.1.Line.*.CallingFeatures" title="Call Options" url="cgi_get_nosubobj?Object=Device.Services.VoiceService.1.VoiceProfile.1.Line.*.CallingFeatures">
            <parameter  name="CallerIDEnable"  type="string" iconicurl="voip_tick_icon" webname="CID Enable" ></parameter>
            <parameter  name="DoNotDisturbEnable"  type="string" iconicurl="voip_tick_icon" webname="DND" ></parameter>
            <parameter  name="CallWaitingEnable"  type="string" iconicurl="voip_tick_icon" webname="Call Waiting" ></parameter>
            <parameter  name="AnonymousCallBlockEnable"  type="string" iconicurl="voip_tick_icon" webname="ACB" ></parameter>
            <parameter  name="MWIEnable"  type="string" iconicurl="voip_tick_icon" webname="MWI" ></parameter>
            <parameter  name="X_VENDOR_COM_CallFwrdVoiceMail"  type="string" iconicurl="voip_tick_icon" webname="Voice Mail" ></parameter>
            <parameter  name="CallForwardUnconditionalEnable"  type="string" iconicurl="voip_tick_icon" webname="CFU" ></parameter>
            <parameter  name="CallForwardOnNoAnswerEnable"  type="string" iconicurl="voip_tick_icon" webname="CFNA" ></parameter>
            <parameter  name="CallForwardOnBusyEnable"  type="string" iconicurl="voip_tick_icon" webname="CFB" ></parameter>
