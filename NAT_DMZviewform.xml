<?xml version="1.0" encoding="UTF-8"?>
<view viewid="NAT_DMZpage">
	<helptext></helptext>
    <header>
    	<image>images/icons/nat.png</image>
        <name>DMZ (Demilitarized Zone)</name>
        <description>If you have a local client PC that cannot run an Internet application properly from behind the NAT firewall, you can open the client up to unrestricted two-way Internet access by defining a virtual DMZ Host.</description>
    </header>
	<object title="Settings" name="Device.NAT.X_LANTIQ_COM_DMZ"  viewtype="form" icon="settings-1">
		<row><col>
		<parameter name="Enable" required="false"  type="checkbox"  writable="true" webname="Enable" maxlen="64"></parameter>
		</col></row>
		<row><col>
		<parameter name="IPAddress" required="false" type="ipv4" writable="true" placeholder="true" webname="IP Address" maxlen="128"></parameter>
		</col><col></col></row>
		<parameter  name="Modify"  type="submitbutton"  routeurl="Apply($event)" webname="Apply" maxlen="64"></parameter>
	</object>
</view>

