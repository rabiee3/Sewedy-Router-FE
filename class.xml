<view>
    <header>
    	<name>Classifier Configuration</name>
        <description>Here classifiers can be configured based on different available interfaces.</description>
    </header>
    <object name="Device.QoS.Classification.*" viewtype="table" Type="string" title="QosClass">
		<parameter name="Alias" type="string" webname="Classifier Name" maxlen="64"></parameter>
		<parameter name="Enable" type="toggle" webname="Enable" maxlen="64"></parameter>
		<parameter name="X_LANTIQ_COM_LowerLayers" type="string" webname="Interface" maxlen="64"></parameter>
		<parameter name="Order" type="string" webname="Order" maxlen="64"></parameter>
		<parameter  name="edit"  action="post"  routeurl="edit($event,'classform')" type="button" webname="Edit" maxlen="64"></parameter>
		<parameter  name="delete"  action="post"   type="button" webname="delete" maxlen="64"></parameter>
		<parameter name="Add" action="post" routeurl="formToOpen('classform')" type="button" webname="Add"></parameter>
	</object>
</view>

