<view viewid="urlfilterviewformpage" translate="urlfilterviewformpage">
<helptext></helptext>
    <header>
        <image>images/icons/firewall.png</image>
        <name>URL Filter Rule</name>
        <description>Configure URLs to block WEB access. </description>
    </header>
    <object title="URL Filter Rules" name="Device.Firewall.X_LANTIQ_COM_URLFilter.Rule.*"  viewtype="form">
        <parameter name="URL"  type="url" webname="URL" maxlen="256" placeholder="true"></parameter>
        <parameter  name="Close"  type="submitbutton" routeurl="Add('urlfilterview')" webname="Close"></parameter>
        <parameter  name="Add"  type="submitbutton"  routeurl="Apply($event,'urlfilterview')" webname="Save"></parameter>
    </object>
</view>

