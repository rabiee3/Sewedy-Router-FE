<view>
    <header>
    	<name>Queue Configuration</name>
        <description>Queues can be configured based on different available interfaces.</description>
    </header>
    <object name="Device.QoS.Queue.*" viewtype="table"  title="QosQueue">
		<parameter name="Alias" type="string"  webname="Queue Name" maxlen="64"></parameter>
		<parameter name="Enable" type="string" webname="Enable" maxlen="64"></parameter>
		<parameter name="Precedence" type="string" webname="Queue Precedence" maxlen="64"></parameter>
		<parameter name="X_LANTIQ_COM_LowerLayers" type="string" webname="Interface" maxlen="64"></parameter>
		<parameter  name="edit"  action="post"  routeurl="edit($event,'queueform')" type="button" webname="Edit" maxlen="64"></parameter>
		<parameter  name="delete"  action="post"   type="button" webname="delete" maxlen="64"></parameter>
		<parameter name="Add" action="post" routeurl="formToOpen('queueform')" type="button" webname="Add"></parameter>
	</object>
</view>

