<?xml version="1.0" encoding="UTF-8"?>
<view>
    <header>
	<image>images/icons/dsl.png</image>
        <name>ATM Channel Info</name>
        <description>This page Displays all ATM channels configured and allows users to delete configured channels.Please avoid deleting ATM channels on which Internet connections are configured.</description>
    </header>    
	<object title="ATM Channels" name="Device.ATM.Link.*" viewtype="table" >
        <parameter  name="DestinationAddress"  type="string"  webname="VPI/VCI" ></parameter>
        <parameter  name="Encapsulation"  type="string"  webname="Encapsulation" ></parameter>
        <parameter  name="LinkType"  type="string"  webname="Link Type" ></parameter>
        <parameter  name="AAL"  type="string"  webname="AAL" ></parameter>
        <parameter  name="Enable"  type="string"  webname="Enable" ></parameter>
        <parameter name="delete" maxlen="64" webname="delete" type="button" action="post"/>
        </object>
</view>
