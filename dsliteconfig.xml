<view>
    <name>DS Lite Configuration</name>
    <object name="Device.DSLite" viewtype="table"  title=">DS Lite Config">
                <parameter name="Enable" type="string" writable="true" webname="Enable" maxlen="64"></parameter>
    <object name="Device.DSLite.InterfaceSetting.*" >
                <parameter name="TunnelInterface" type="dropdown" Value="" writable="true" webname="Interface" maxlen="64"></parameter>
                <parameter name="EndpointAssignmentPrecedence" type="dropdown" Value="" writable="true" webname="Config Modes" maxlen="64"></parameter>
                <parameter name="X_LANTIQ_COM_MTU" type="string"  writable="true" webname="MTU" maxlen="64"></parameter>
                <parameter name="EndpointAddress" type="string" writable="true" webname="DS Lite Remot IPV6 Add" maxlen="64"></parameter>
                <parameter name="X_LANTIQ_COM_TunnelIP" type="string" writable="true" webname="DS Tunnel IPV4 Add" maxlen="64"></parameter>
                <parameter name="X_LANTIQ_COM_Mask" type="string" writable="true" webname="Subnet Mask" maxlen="64"></parameter>
                <parameter name="X_LANTIQ_COM_PortRange" type="string" writable="true" webname="Lw4o6 Port Range" maxlen="64"></parameter>
     </object>
                <parameter  name="delete"  action="post"  type="button" webname="delete" maxlen="64"></parameter>
                <parameter name="Add" action="post" routeurl="formToOpen('dsliteconfigform')" type="button" webname="Add"></parameter>
                <parameter name="edit" action="post" routeurl="edit($event,'dsliteconfigform')" type="button" webname="Edit" maxlen="64"></parameter>
        </object>
</view>
